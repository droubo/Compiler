// add
x = 1; 	   // 1 const
x = 1 + 1; // 2 const
x = 1 + y; // const , var
x = y + 1; // var , const
x = w + y; // var , var

// sub
x = 1 - 1; // 2 const
x = 1 - y; // const , var
x = y - 1; // var , const
x = w - y; // var , var

// div
x = 1 / 1; // 2 const
x = 1 / y; // const , var
x = y / 1; // var , const
x = w / y; // var , var

// mul
x = 1 * 1; // 2 const
x = 1 * y; // const , var
x = y * 1; // var , const
x = w * y; // var , var

// passed //


